datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

// model de Us√∫ario
model User {
  id         String @id @default(uuid())
  github_id  Int
  avatar_url String
  login      String
  name       String @ignore


  @@map("users")
}
